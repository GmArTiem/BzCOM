<Window x:Class="BzCOMWpf.ChatMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BzCOMWpf"
        xmlns:wf1="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        mc:Ignorable="d"
        Title="ChatMessage" Height="460" Width="380" Closing="Window_Closing">
    <Border>
        <Grid>

            <TextBox x:Name="TextBoxMessage" HorizontalAlignment="Left" Height="23" Margin="86,378,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
            <Button x:Name="ButtonSend" Content="Send" HorizontalAlignment="Left" Margin="235,378,0,0" VerticalAlignment="Top" Width="75" Click="ButtonSend_Click_1"/>
            <WindowsFormsHost HorizontalAlignment="Left" Height="351" Margin="10,22,0,0" VerticalAlignment="Top" Width="352">
                <wf1:WebBrowser x:Name="Webbrowser"/>
            </WindowsFormsHost>
            
        </Grid>
    </Border>
</Window>
